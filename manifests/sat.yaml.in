apiVersion: manifests/v1beta1
metadata:
  name: sat-manifest
spec:
  charts:
    - name: sat-cfs-install
      namespace: services
      version: @SAT_CFS_HELM_VERSION@
      values:
        cray-import-config:
          catalog:
            image:
              # Use the version the SAT product put into Nexus
              repository: registry.local/cray/cray-product-catalog
              tag: @CPCU_VERSION@
          config_image:
            image:
              tag: @SAT_CFS_DOCKER_VERSION@
          import_job:
            CF_IMPORT_PRODUCT_VERSION: @RELEASE_VERSION@
